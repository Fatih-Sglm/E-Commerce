@model IEnumerable<EntityLayer.Concrete.Adress>

<div class="tab-pane fade" id="address-tab" role="tabpanel" aria-labelledby="address-nav">
    <h4>Kayıtlı Adresleriniz  &nbsp; &nbsp;   <a class="btn" href="/Customer/NewAdress/">Yeni Adres Ekleme</a></h4>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-6">
                <h5>@item.Title</h5>
                <p>@item.detail @item.HouseNumber @item.City/@item.Country</p>
                @*<p>Mobile:@item.Customer.PhoneNumber</p>*@
                <a class="btn" href="/Customer/EditAdress/@item.Id">Edit Address</a>
                <br />
            </div>
        }

    </div>  
</div>